<script src="https://sg-rightmobilite--preprod.sandbox.my.site.com/lightning/lightning.out.js"></script>
<div data-lightning-out="true"> </div>
<script >
const appName='c:DemandeEspaceMobilit';
const componentName='c:component2Buttons';
const lightningEndPoint='https://sg-rightmobilite--preprod.sandbox.my.site.com';
const targetElement=document.querySelector("[data-lightning-out]");
const componentAttributes={

};

$Lightning.use(
	appName,
	function(){
		$Lightning.createComponent(
			componentName,
			componentAttributes,
           targetElement  ,
            function(cmp){
				console.log('lightning component created ');
			}


		);
	},
	lightningEndPoint
);
</script>
